[
  {
    "id": "Image Tag",
    "ErrorTooltip": "Image tag is missing alt attribute or text within the alt attribute",
    "RegEx": [
      "<img(?![^>]*alt=\".+?\")[^>]*?/>"
    ]
  },
  {
    "id": "Radio Button No Name",
    "ErrorTooltip": "MAS requires a name attribute with all radio buttons",
    "RegEx": [
      "<input(?![^>]*\\bname=[\"].+?[\"])(?![^>]*\\btype=[\"]radio[\"])[^>]*?>"
    ]
  },
  {
    "id": "Radio Button No Label",
    "ErrorTooltip": "MAS requires a label with all radio buttons",
    "RegEx": [
      "<input\\s+?(?=.*type=\"radio\")(?=.*name=\".+?\").*/>",
      "<label.+>.+</label>"
    ]    
  },
  {
    "id": "CheckBox No Label",
    "ErrorTooltip": "MAS requires a label with all radio buttons",
    "RegEx": [
      "<input\\s+?(?=.*type=\"checkbox\")(?=.*name=\".+?\").*/>",
      "<label.+>.+</label>"
    ]    
  },
  {
    "id": "CaptionTable",
    "ErrorTooltip": "MAS requires a label with all radio buttons",
    "RegEx": [
      "<table>(\n|.)*</table>",
      "<caption\\s*>.+?</caption>"
    ]    
  },
  {
    "id": "TableRow",
    "ErrorTooltip": "MAS requires a label with all radio buttons",
    "RegEx": [
      "<tr>(\n|.)*?</tr>",
      "<th(?=.*scope=\".+?\")[^>]*?>.+?</th>"
    ]    
  }
]
